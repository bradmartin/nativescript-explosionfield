{
	"name": "nativescript-explosionfield",
	"version": "1.0.1",
	"description": ":bomb: NativeScript plugin to create an exploding particle animation :bomb:",
	"main": "explosion.js",
	"nativescript": {
		"platforms": {
			"android": "1.7.0"
		}
	},
	"scripts": {
		"build": "tsc",
		"demo.android": "npm run preparedemo; cd demo; tns run android",
		"preparedemo": "npm run build; cd demo; tns plugin remove nativescript-yourplugin; tns plugin add ..; tns install",
		"setup": "cd demo; npm install; cd ..; npm run build; cd demo; tns plugin add ..; cd .."
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/bradmartin/nativescript-explosionfield.git"
	},
	"keywords": [
		"NativeScript",
		"JavaScript",
		"Android",
		"Animation",
		"Explosion Field",
		"bradmartin",
		"thorum"
	],
	"author": {
		"name": "Brad Martin",
		"email": "bradwaynemartin@gmail.com"
	},
	"contributors": [
		{
			"name": "Josh Sommer",
			"email": "JoshDSommer@gmail.com"
		}
	],
	"bugs": {
		"url": "https://github.com/bradmartin/nativescript-explosionfield/issues"
	},
	"license": {
		"type": "MIT",
		"url": "https://github.com/bradmartin/nativescript-explosionfield/blob/master/LICENSE"
	},
	"homepage": "https://github.com/bradmartin/nativescript-explosionfield",
	"readmeFilename": "README.md",
	"devDependencies": {
		"typescript": "^1.8.7"
	}
}